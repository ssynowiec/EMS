    "@fastify/sensible": "^5.2.0",
    "@prisma/client": "5.2.0",
    "fastify": "^4.23.0",
    "fastify-cli": "^5.8.0",
    "fastify-plugin": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.6.0",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-config-custom": "workspace:*",
    "prisma": "^5.2.0",
    "tap": "^16.1.0",
    "tsx": "^3.12.8",
    "typescript": "^5.2.2"
  }
